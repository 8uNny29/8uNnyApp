<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>HISS TKJ</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" type="text/css" href="styles.css" />
    <link rel="stylesheet" type="text/css" href="assets/css/gaya.css" />

    <!-- Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />

    <!-- Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <!-- link CDN -->
    <link
      rel="stylesheet"
      href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
    />

    <!-- LINK FONT  -->
    <!-- <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> -->
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@100&family=Secular+One&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <%- include('../navbar') %>

    <div class="page-header">
      <h2>HISS TKJ</h2>
      <p>Himpunan Siswa Siswi TKJ</p>
    </div>

    <div class="divisi" id="dev-p1">
      <h2>Aktivitas HISS TKJ</h2>
      <div class="pro-cont">
        <% aktivitas.forEach(function(row) { %>
        <div class="dev">
          <img
            src="assets/images/Untitled-2.png"
            alt="<%= row.nama_aktivitas %>"
          />
          <!-- <img
            src="assets/images/divisi/<%= row.gambar %>"
            alt="<%= row.nama_aktivitas %>"
          /> -->
          <div class="des">
            <h4><%= row.nama_aktivitas %></h4>
            <span><%= row.tanggal %></span>
            <h5><%= row.deskripsi %></h5>
          </div>
        </div>
        <% }); %>
      </div>
    </div>

    <div class="baner1" id="dev-p2">
      <h2>PUSH YOUR LIMIT!</h2>
      <h4>HISS TKJ</h4>
    </div>

    <%- include('../footer') %>
    <script src="scrip.js"></script>
  </body>
</html>
