<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>HISS TKJ</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" type="text/css" href="styles.css" />
    <link rel="stylesheet" type="text/css" href="assets/css/gaya.css" />

    <!-- Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />

    <!-- Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <!-- link CDN -->
    <link
      rel="stylesheet"
      href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
    />

    <!-- LINK FONT  -->
    <!-- <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> -->
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@100&family=Secular+One&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <%- include('../navbar') %>

    <div id="layoutSidenav_content">
      <main>
        <div class="page-header">
          <h2>Blog About PALADA</h2>
        </div>

        <section class="blog">
          <% blog.forEach(function(row) { %>
          <div class="blog-box">
            <div class="blog-img">
              <img
                src="admin/img/blog/<%= row.gambar %>"
                alt="<%= row.judul %>"
              />
            </div>
            <div class="blog-details">
              <h4><%= row.judul %></h4>
              <p><%= row.konten %></p>
              <a href="<%= row.link %>">Selengkapnya</a>
            </div>
          </div>
          <% }); %>
        </section>
      </main>
    </div>

    <div class="baner1" id="dev-p2">
      <h2>PUSH YOUR LIMIT!</h2>
      <h4>HISS TKJ</h4>
    </div>

    <%- include('../footer') %>
    <script src="scrip.js"></script>
  </body>
</html>
